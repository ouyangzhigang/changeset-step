#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/oker/project/changeset-step/node_modules/.pnpm/release-it@17.10.0/node_modules/release-it/bin/node_modules:/Users/oker/project/changeset-step/node_modules/.pnpm/release-it@17.10.0/node_modules/release-it/node_modules:/Users/oker/project/changeset-step/node_modules/.pnpm/release-it@17.10.0/node_modules:/Users/oker/project/changeset-step/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/oker/project/changeset-step/node_modules/.pnpm/release-it@17.10.0/node_modules/release-it/bin/node_modules:/Users/oker/project/changeset-step/node_modules/.pnpm/release-it@17.10.0/node_modules/release-it/node_modules:/Users/oker/project/changeset-step/node_modules/.pnpm/release-it@17.10.0/node_modules:/Users/oker/project/changeset-step/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../release-it@17.10.0/node_modules/release-it/bin/release-it.js" "$@"
else
  exec node  "$basedir/../../../../../../release-it@17.10.0/node_modules/release-it/bin/release-it.js" "$@"
fi
